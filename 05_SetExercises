>>> my_set = set()
>>> type(my_set)
<class 'set'>
>>> my_set = {1,2,3}
>>> my_set
{1, 2, 3}
>>> my_set.add(4)
>>> my_set
{1, 2, 3, 4}
>>> my_set.remove(2)
>>> my_set
{1, 3, 4}
>>> 2 in my_set
False
>>> my_set.add(3) # this won't change the set since there can only be unique values
>>> my_set
{1, 3, 4}
>>> my_other_set = {1,2,3}
>>> my_set.union(my_other_set)
{1, 2, 3, 4}
>>> my_set.difference(my_other_set)
{4}
>>> my_set.intersection(my_other_set)
{1, 3}
>>>
